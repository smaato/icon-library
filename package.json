{
  "name": "icon-library",
  "version": "1.1.0",
  "description": "Smaato's icon library",
  "scripts": {
    "compileScss": "sass --no-source-map scss/index.scss css/index.css",
    "createInlineIcons": "node createInlineIcons.js",
    "lint": "eslint .",
    "precommit": "npm run lint",
    "start": "npm run createInlineIcons && npm run compileScss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smaato/icon-library.git"
  },
  "license": "MIT",
  "dependencies": {
    "sass": "^1.75.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11"
  }
}
